GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MCAnimationView" "${PODS_ROOT}/Headers/Public/MCUIImageAdvanced" "${PODS_ROOT}/Headers/Public/MGImageUtilities"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/MCAnimationView" "$PODS_CONFIGURATION_BUILD_DIR/MCUIImageAdvanced" "$PODS_CONFIGURATION_BUILD_DIR/MGImageUtilities"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MCAnimationView" -isystem "${PODS_ROOT}/Headers/Public/MCUIImageAdvanced" -isystem "${PODS_ROOT}/Headers/Public/MGImageUtilities"
OTHER_LDFLAGS = $(inherited) -ObjC
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
